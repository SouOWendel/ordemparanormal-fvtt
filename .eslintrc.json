{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "google", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"valid-jsdoc": ["off"],
		"no-undef": ["off"],
		"prefer-rest-params": ["off"]
	}
}
